" cerebnismus's dotfiles v.1.2

" CoPilot in VIM
" git clone https://github.com/github/copilot.vim.git \
"  ~/.vim/pack/github/start/copilot.vim

" colorscheme desert

" when delete not work
set backspace=indent,eol,start

syntax enable

set number
set smartindent
set tabstop=4
set shiftwidth=4
set expandtab

set showmatch
set wrap
set mouse=a
set incsearch
set ignorecase
set smartcase
set ruler
set clipboard=unnamedplus
set path+=**
set undofile
set wildmenu
